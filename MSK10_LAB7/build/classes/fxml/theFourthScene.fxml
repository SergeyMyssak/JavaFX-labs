<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="340.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TheFourthController">
   <children>
      <Label layoutX="20.0" layoutY="20.0" text="Какое у вас образование:">
         <font>
            <Font name="Arial" size="19.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="20.0" layoutY="77.0" prefWidth="150.0" />
      <Label fx:id="label" layoutX="18.0" layoutY="274.0" text="Выбрано: Неполное среднее">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Label>
      <Button fx:id="buttonPrev" layoutX="78.0" layoutY="301.0" mnemonicParsing="false" onAction="#buttonPrevAction" prefWidth="80.0" text="Назад">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Button>
      <Button fx:id="buttonNext" layoutX="212.0" layoutY="301.0" mnemonicParsing="false" onAction="#buttonNextAction" prefWidth="80.0" text="Вперед">
         <font>
            <Font name="Arial" size="12.0" />
         </font></Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="182.0" layoutY="77.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Eduaction.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
